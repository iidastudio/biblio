{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "styles": {
    "blocks": {
      "core/post-navigation-link": {
        "spacing": {
          "blockGap": "0"
        },
        "typography": {
          "fontSize": "16px"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--biblio-text)"
            },
            "spacing": {
              "padding": {
                "top": "0",
                "right": "0.5em",
                "bottom": "0",
                "left": "0.5em"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      }
    }
  }
}