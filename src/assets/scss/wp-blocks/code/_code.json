{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "styles": {
    "blocks": {
      "core/code": {
        "color": {
          "background": "var(--wp--preset--color--biblio-text)",
          "text": "var(--wp--preset--color--biblio-background)"
        },
        "typography": {
          "fontFamily": "\"SFMono-Regular\",Consolas,\"Liberation Mono\",Menlo,Courier,monospace",
          "fontSize": "14.4px",
          "lineHeight": "1.5"
        },
        "spacing": {
          "padding": {
            "top": ".5em",
            "right": "1em",
            "bottom": ".5em",
            "left": "1em"
          }
        },
        "border": {
          "style": "solid",
          "width": "1px",
          "color": "var(--wp--preset--color--biblio-support-text)"
        }
      }
    }
  }
}