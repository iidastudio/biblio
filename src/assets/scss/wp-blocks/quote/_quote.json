{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "styles": {
    "blocks": {
      "core/quote": {
        "color": {
          "background": "var(--wp--preset--color--biblio-support-foreground)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--50)",
            "bottom": "var(--wp--preset--spacing--50)",
            "left": "var(--wp--preset--spacing--60)",
            "right": "var(--wp--preset--spacing--60)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--sm)",
          "lineHeight": "1.5"
        },
        "elements": {
          "cite": {
            "spacing": {
              "margin": {
                "top": "2em"
              }
            },
            "typography": {
              "fontSize": "var(--wp--preset--font-size--xxs)",
              "fontStyle": "italic"
            }
          }
        }
      }
    }
  }
}