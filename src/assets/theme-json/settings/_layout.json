{
  "settings": {
    "layout": {
      "contentSize": "var(--wp--custom--layout--content-size)",
      "wideSize": "var(--wp--custom--layout--wide-size)"
    },
    "custom": {
      "layout": {
        "post": "624px",
        "wide": "calc(1280px - var(--wp--style--root--padding-left) * 2)",
        "page": "calc(1280px - var(--wp--style--root--padding-left) * 2)",
        "pageWide": "calc(1920px - var(--wp--style--root--padding-left) * 2)",
        "contentSize": "min(100%, var(--wp--custom--layout--post))",
        "wideSize": "min(100%, var(--wp--custom--layout--wide))",
        "pageContentSize": "min(calc(100% - var(--wp--style--root--padding-left) * 2), var(--wp--custom--layout--page))",
        "pageWideSize": "min(calc(100% - var(--wp--style--root--padding-left) * 2), var(--wp--custom--layout--page-wide))"
      }
    }
  }
}