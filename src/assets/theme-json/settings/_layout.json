{
  "settings": {
    "layout": {
      "contentSize": "var(--wp--custom--layout--content-size)",
      "wideSize": "var(--wp--custom--layout--wide-size)"
    },
    "custom": {
      "layout": {
        "post": "40rem",
        "wide": "calc(1280px - var(--wp--style--root--padding-left) * 2)",
        "extraWide": "calc(1920px - var(--wp--style--root--padding-left) * 2)",
        "contentSize": "min(100%, var(--wp--custom--layout--post))",
        "wideSize": "min(100%, var(--wp--custom--layout--wide))",
        "extraWideSize": "min(100%, var(--wp--custom--layout--extra-wide))"
      }
    }
  }
}